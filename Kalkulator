print("kalkultor")
print("1=dodawanie")
print("2=odjmowanie")
print("3=mnożenie")
print("4=dzielenie")
a=int(input("podaj a"))
b=int(input("podaj b"))
p=input("podaj działanie")
if p=="1":
    print(int(a+b))
elif p=="2":
    print(int(a-b))
elif p=="3":
    print(int(a*b))
elif p=="4":
    if b!=0:
        print(int(a/b))
    else:
        print("")
        print("kys")
